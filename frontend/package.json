{
  "name": "vite-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:server": "nodemon",
    "build": "tsc && vite build",
    "build:server": "rollup -c",
    "preview": "vite preview",
    "server:prod": "NODE_ENV=production node ./distServer/index.js",
    "types": "npm run generate-types && npm run lint --fix ./src/types/external && npm run prettier --write ./src/types/external",
    "generate-types": "openapi-typescript \"../backend/travel-api/api/specs/**/*.yaml\" --output ./src/types/external/",
    "lint": "eslint --fix",
    "prettier": "prettier --write",
    "extract": "formatjs extract",
    "compile": "formatjs compile"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@js-temporal/polyfill": "^0.4.4",
    "@mantine/core": "^6.0.17",
    "@mantine/form": "^6.0.17",
    "@mantine/hooks": "^6.0.17",
    "@react-google-maps/api": "^2.19.2",
    "@tanstack/react-query": "^4.35.0",
    "focus-trap-react": "^10.2.1",
    "lucide-react": "^0.263.1",
    "react": "^18.2.0",
    "react-aria": "^3.26.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-intl": "^6.4.4",
    "react-router-dom": "^6.8.0"
  },
  "devDependencies": {
    "@fastify/compress": "^6.4.0",
    "@fastify/static": "^6.10.2",
    "@formatjs/cli": "^6.1.3",
    "@rollup/plugin-commonjs": "^25.0.3",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-typescript": "^11.1.2",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/react-helmet": "^6.1.6",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "@vitejs/plugin-react": "^3.0.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-formatjs": "^4.10.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "fastify": "^4.21.0",
    "nodemon": "^3.0.1",
    "npm": "^9.8.1",
    "openapi-typescript": "^6.5.0",
    "prettier": "^2.8.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "vite": "^4.0.0",
    "vite-plugin-checker": "^0.6.1"
  }
}
